        <StackPanel HorizontalAlignment="Left" Height="200" Margin="105,115,0,-290" VerticalAlignment="Top" Width="340" Background="{DynamicResource {x:Static SystemColors.ScrollBarBrushKey}}"/>
        <Separator HorizontalAlignment="Left" Height="140" Margin="325,205,0,-319" VerticalAlignment="Top" Width="265" Background="#FF682A2A"/>

    </Grid>
