        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:FunSeqWPF"
        mc:Ignorable="d"
        Title="ImageSeries" Height="450" Width="800">
    <Grid Margin="0,0,0,392" VerticalAlignment="Top" Background="{DynamicResource {x:Static SystemColors.ControlDarkBrushKey}}">
